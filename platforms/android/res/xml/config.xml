<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dsjn.s2p.prod" version="0.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PushNotification">
        <param name="android-package" value="com.adobe.phonegap.push.PushPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="android-package" value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
    </feature>
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <preference name="fullscreen" value="false" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/android/ldpi.png" />
    <icon density="mdpi" src="res/android/mdpi.png" />
    <icon density="hdpi" src="res/android/hdpi.png" />
    <icon density="xhdpi" src="res/android/xhdpi.png" />
    <splash density="port-hdpi" src="res/android/screen-hdpi-portrait.png" />
    <splash density="port-ldpi" src="res/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" src="res/android/screen-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="res/android/screen-xhdpi-portrait.png" />
    <name>Konecto</name>
    <description>
        Konecto App
    </description>
    <author email="denis.jacquemin@gmail.com" href="http://tobedefine.be">
        Denis Jacquemin
    </author>
    <content src="index.html" />
    <access origin="*://s2p-api-demo.herokuapp.com/*" />
    <access origin="*://s2p-api-prod.herokuapp.com/*" />
    <access origin="*://s2p-demo.s3-eu-west-1.amazonaws.com/*" />
    <access origin="https://*.google-analytics.com/*" />
</widget>
